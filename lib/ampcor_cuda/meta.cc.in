// get the declarations
#include "meta.h"


// version info
auto
ampcor::cuda::meta::
version() -> version_t
{
    return version_t {
        AMPCOR_CUDA_major,
        AMPCOR_CUDA_minor, AMPCOR_CUDA_MICRO,
        AMPCOR_CUDA_revision
    };
}


// compilation date
auto
ampcor::meta::
date() -> string_t
{
    return AMPCOR_date;
}


