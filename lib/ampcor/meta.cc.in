// get the declarations
#include "meta.h"


// version info
auto
ampcor::meta::
version() -> version_t
{
    return version_t { AMPCOR_major, AMPCOR_minor, AMPCOR_MICRO, AMPCOR_revision };
}


// compilation date
auto
ampcor::meta::
date() -> string_t
{
    return AMPCOR_date;
}


